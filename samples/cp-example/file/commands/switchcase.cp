// switchcase

//https://stackoverflow.com/questions/20031147/using-in-a-case-statement-why

switch(5) {
case 0:
case 1:
    print(1);
    break;
case 2: {
    print(2);
}   
    break;
case 3:
    print(3);
case 4:
    print(4);
    break;
case 5:
    print(5);
    break;
default:
    print("default");
    break;
}
